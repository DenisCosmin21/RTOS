#include "led.h"

#define GPIODEN 		(1U<<3)
#define LED_PIN 		(1U<<12);
void led_init(void){
	/*Enable clock access to led port ( port D)*/

	RCC->AHB1ENR |= GPIODEN;

	/*Set LED pin as output pin*/
	//24 -255

	GPIOD->MODER |= (1U<<24);
	GPIOD->MODER  &= ~(1U<25);
}


void led_on(void){

	/*Set led pin HIGH(PD12)*/
	GPIOD->ODR |= LED_PIN;
}

void led_off(void){

	/*Set led pin LOW(PD12)*/
}
